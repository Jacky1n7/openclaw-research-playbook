{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "OpenClaw Cron Receipt",
  "type": "object",
  "required": ["ts", "idempotency_key", "inputs_hash", "decision", "side_effects", "validity_checked_at"],
  "properties": {
    "ts": {"type": "integer", "description": "Unix seconds"},
    "idempotency_key": {"type": "string"},
    "inputs_hash": {"type": "string"},
    "decision": {"type": "string"},
    "side_effects": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["type", "target"],
        "properties": {
          "type": {"type": "string"},
          "target": {"type": "string"},
          "status": {"type": "string"},
          "details": {"type": "object"}
        }
      }
    },
    "validity_checked_at": {"type": "integer"},
    "notes": {"type": "string"}
  }
}
